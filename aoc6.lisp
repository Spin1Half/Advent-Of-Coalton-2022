(in-package :coalton-user)



(coalton-toplevel

  
  (declare first-unique-n (String -> UFIX -> UFIX -> UFIX))
  (define (first-unique-n str n i)
    (if (unique-n-p (String:substring str i (+ i n)) n)
        (+ i n)
        (first-unique-n str n (1+ i))))


  (declare unique-n-p (String -> UFIX -> Boolean))
  (define (unique-n-p str n)
    (if (== n 2)
        (/= (fromsome "" (string:ref str 0)) (fromsome "" (string:ref str 1)))
        (if (String:substring? (String:substring str 0 1) (String:substring str 1 n))
            False
            (unique-n-p (String:substring str 1 n) (1- n))))))




